{
    "name": "koa-file-cache",
    "version": "0.3.0",
    "description": "Koa middleware to cache expensive requests (e.g. remote, nested DB, etc) to disk",
    "repository": {
        "type": "git",
        "url": "https://github.com/jonbo/koa-file-cache"
    },
    "main": "index.js",

    "keywords": [
        "koa",
        "middleware",
        "file",
        "cache",
        "disk"
    ],

    "dependencies": {
        "debug": "*"
    },
    "peerDependencies": {
        "koa": ">0.3.0"
    },
    "devDependencies": {
        "koa": "~0.21.0",
        "koa-compress": "~1.0.8",
        "mocha": "~2.2.5",
        "should": "~3.1.2",
        "supertest": "~0.9.0",
        "mkdirp": "0.5.1",
        "rimraf": "2.4.2"
    },
    "license": "MIT",

    "scripts": {
        "pretest": "mkdirp ./test/output",
        "test": "mocha --require should --harmony-generators --reporter spec --bail",
        "posttest": "rimraf ./test/output"
    }

}
